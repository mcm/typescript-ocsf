  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "download": "tsx scripts/download-schemas.ts",
    "generate": "tsx scripts/generate.ts",
    "build": "tsup && tsc --emitDeclarationOnly",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "biome check src/ tests/ scripts/",
    "lint:fix": "biome check --write src/ tests/ scripts/",
    "format": "biome format --write src/ tests/ scripts/",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.0",
    "@vitest/coverage-v8": "^1.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ocsf",
    "cybersecurity",
    "schema",
    "zod",
    "validation",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mcm/typescript-ocsf.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
